scalar Date

type Query {
    countries: [Country]
    country(id: ID!): Country
}

type Mutation {
    addCountry(input: CountryInput!): Country!
    updateCountry(id: ID!, input: CountryInput!): Country!
}

input CountryInput {
    countryName: String!
    countryCode: String!
}

type Country {
    id: ID!,
    countryName: String!,
    countryCode: String!,
    independenceDay: Date!
}